{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sessions"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase2'"
		},
		"LSblobSA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSblobSA'"
		},
		"session25_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'session25'"
		},
		"session27linkblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'session27linkblob'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sessiondestination.dfs.core.windows.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlcloud123.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldatabase123"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pavuluri123"
		},
		"AzureSqlDatabase2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlcloud123.database.windows.net"
		},
		"AzureSqlDatabase2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldatabase123"
		},
		"AzureSqlDatabase2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pavuluri123"
		},
		"lskeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mounikavault.vault.azure.net/"
		},
		"lssqlkeyvault_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlcloud123.database.windows.net"
		},
		"lssqlkeyvault_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldatabase123"
		},
		"lssqlkeyvault_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pavuluri123"
		},
		"event trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/019ead8b-377e-45fa-99af-8d29416dffec/resourceGroups/mounika/providers/Microsoft.Storage/storageAccounts/sessionblob"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sessions_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"AzureSqlDatabase2_password": {
						"value": "[parameters('AzureSqlDatabase2_password')]"
					},
					"LSblobSA_connectionString": {
						"value": "[parameters('LSblobSA_connectionString')]"
					},
					"session25_connectionString": {
						"value": "[parameters('session25_connectionString')]"
					},
					"session27linkblob_connectionString": {
						"value": "[parameters('session27linkblob_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_userName')]"
					},
					"lskeyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('lskeyvault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sessions_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lssqlkeyvault_properties_typeProperties_server": {
						"value": "[parameters('lssqlkeyvault_properties_typeProperties_server')]"
					},
					"lssqlkeyvault_properties_typeProperties_database": {
						"value": "[parameters('lssqlkeyvault_properties_typeProperties_database')]"
					},
					"lssqlkeyvault_properties_typeProperties_userName": {
						"value": "[parameters('lssqlkeyvault_properties_typeProperties_userName')]"
					},
					"event trigger_properties_typeProperties_scope": {
						"value": "[parameters('event trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sessions_ArmTemplate_0')]"
			]
		}
	]
}